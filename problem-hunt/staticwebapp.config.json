{
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "globalHeaders": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS"
    },
    "routes": [
      {
        "route": "/api/*",
        "allowedRoles": ["anonymous"]
      }
    ],
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html"
      }
    },
    "buildCommand": "echo 'No build required'",
    "outputLocation": "."
  }